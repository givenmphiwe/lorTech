# PostgreSQL Database Configuration
DB_HOST=your_database_host           # e.g., localhost or a production server IP
DB_PORT=5432                         # Default PostgreSQL port is 5432
DB_DATABASE=your_database_name       # e.g., kota_db
DB_USER=your_database_user           # e.g., postgres
DB_PASSWORD=your_database_password   # e.g., secure_password

# AWS S3 Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_id     # Your AWS Access Key ID
AWS_SECRET_ACCESS_KEY=your_aws_secret_key    # Your AWS Secret Access Key
AWS_REGION=your_aws_region                   # e.g., us-east-1, eu-west-2
S3_BUCKET_NAME=your_s3_bucket_name           # Your S3 bucket name
